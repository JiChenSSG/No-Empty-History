{
    "name": "no empty history",
    "description": "no empty history",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": [
        "history",
        "sessions",
        "storage"
    ], 
    "action": {
        "default_popup": "view.html",
        "default_icon": "icon.png"
    },
    "background": {
        "service_worker": "background.js"
    }
}