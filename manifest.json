{
    "name": "no empty history",
    "description": "no empty history",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": [
        "history",
        "sessions",
        "storage"
    ],
    "content_scripts": [
        {
            "matches": [
                "http://*/*",
                "https://*/*"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_start"
        }
    ],
    "action": {
        "default_popup": "main.html",
        "default_icon": "icon.png"
    },
    "background": {
        "service_worker": "background.js"
    }
}